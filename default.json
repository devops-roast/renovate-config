{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:best-practices",
    ":dependencyDashboard",
    "docker:enableMajor",
    ":enablePreCommit",
    "github>devops-roast/renovate-config:commit-message",
    "github>devops-roast/renovate-config:pr-labels",
    "github>devops-roast/renovate-config:semantic-commits",
    "github>devops-roast/renovate-config:custom-managers",
    "github>devops-roast/renovate-config:noise-reduction",
    "github>devops-roast/renovate-config:lockfile-maintenance",
    "github>devops-roast/renovate-config:grouping"
  ],
  "onboarding": false,
  "requireConfig": "optional",
  "timezone": "Europe/Warsaw",
  "dependencyDashboardTitle": "Renovate Dashboard",
  "suppressNotifications": ["prIgnoreNotification"],
  "commitBodyTable": true,
  "rebaseWhen": "conflicted",
  "pinDigests": false
}
